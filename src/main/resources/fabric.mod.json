  "name": "MasterMind",
  "description": "${description}",
  "authors": [
    "Snowii"
  ],
  "contact": {
    "repo": "https://github.com/Snowiiii/MasterMind"
  },
  "license": "GNU General Public License v3.0",
  "icon": "assets/mastermind/icon.png",
